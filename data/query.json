{
  "queryFunction":"employee",
  "fileds":[
    {
      "name":"name",
      "label":"姓名","conditions":[
        {"name":"equal","label":"是","value":"=","placeholder":"姓名", "type":"text"},
        {"name":"equal","label":"包含","value":"like","placeholder":"姓名", "type":"text"},
        {"name":"equal","label":"不包含","value":"notLike","placeholder":"姓名", "type":"text"},
        {"name":"equal","label":"开头是","value":"likeStart","placeholder":"姓名", "type":"text"},
        {"name":"equal","label":"结尾是","value":"likeEnd","placeholder":"姓名", "type":"text"}
      ]
    },
    {
      "name":"name",
      "label":"性别","conditions":[
        {"name":"gender","label":"男","value":"1"},
        {"name":"gender","label":"女","value":"2"}
      ]
    },
    {
      "name":"name",
      "label":"访问时间","conditions":[
        {"name":"lastDate","label":"最近","value":"30","unitLable":"天","placeholder":"30", "type":"number"},
        {"name":"dateRange","label":"时间段","placeholder":"选择时间段","between":"90","maxDate":"2016-09-07","minDate":"2016-08-15", "type":"dateRange"}
      ]
    },
    {
      "name":"age",
      "label":"年龄","conditions":[
        {"name":"number","label":">=","value":">=","maxValue":"100","minValue":"17","type":"number","placeholder":"年龄"},
        {"name":"numberRange","label":"=","value":"=","between":"90","maxValue":"100","minValue":"17","type":"numberRange","placeholder":"年龄段"}
      ]
    }
  ]
}